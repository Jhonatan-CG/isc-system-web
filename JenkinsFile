pipeline {
    agent any
    tools {
        nodejs '20'
    }
    stages {
        stage('Clone Project') {
            steps {
                git branch: 'qa', url: 'https://github.com/Jhonatan-CG/isc-system-web.git'
            }
        }
        stage('Install Dependencies') {
            steps {
                sh 'npm install'
            }
        }
        stage('Build') {
           steps {
                sh 'npm install --legacy-peer-deps'
            }

        }
    }
}